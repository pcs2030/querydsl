input Filters {
    entity: String,
    criteria: [FilterCriteria]
}

input FilterCriteria {
    key: String,
    value: String
}

input Pagging {
    startIndex: Int,
    pageSize: Int
}

type Query {
    parents(name: String): [ParentEntity]
    getData(criteria: [FilterCriteria]): [ParentEntity]
}

type ParentEntity {
    id: ID
    name: String
    childEntities: [ChildEntity!]!
}


type ChildEntity {
    id: ID
    parentID: ID
    name: String!
}

